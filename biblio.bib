@inbook{long2017bookRedundancyChap5,
	publisher = {John Wiley \& Sons, Ltd},
	author =    {Jin, Long; Zhang, Yunong},
	isbn = {9781119381440},
	title = {Redundancy Resolution via QP Approach and Unification},
	booktitle = {Robot Manipulator Redundancy Resolution},
	chapter = {5},
	pages = {47-66},
	doi = {https://doi.org/10.1002/9781119381440.ch5},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119381440.ch5},
	eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119381440.ch5},
	year = {2017},
	keywords = {Redundant robot manipulators, quadratic programming (QP), QP unification, Joint physical limits, Performance indices, Dual neural network, Numerical algorithm E47, Numerical algorithm 94LVI},
	abstract = {Summary This chapter presents the results regarding the unification of redundancy resolution of robot manipulators via quadratic programming (QP). The presented QP formulation is general in the sense that it incorporates equality, inequality, and bound constraints simultaneously. This QP formulation covers the online avoidance of joint physical limits and environmental obstacles, as well as the optimization of various performance indices. Every term is endowed with clear physical meaning and utility. Motivated by the real-time solution to such robotic problems, some QP online solutions are briefly reviewed. That is, traditional QP optimization routines, compact QP method, dual neural network as a QP solution, linear variational inequality (LVI)-aided primal-dual neural network (LVIAPDNN) as a QP solution, and two numerical algorithms E47 and 94LVI. Note that the presented LVIAPDNN has simple piecewise-linear dynamics, does not entail real-time matrix inversion, and can also provide joint-acceleration information for manipulator torque control in the velocity-level redundancy-resolution schemes. The QP-based unification of robots' redundancy resolution is substantiated by a large number of computer simulation results based on PUMA560, PA10, and planar robot manipulators.}}

