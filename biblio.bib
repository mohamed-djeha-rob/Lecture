@Article{shi2022machines,
	AUTHOR = {Shi, Yapeng and He, Xiaolong and Zou, Wenpeng and Yu, Bin and Yuan, Lipeng and Li, Mantian and Pan, Gang and Ba, Kaixian},
	TITLE = {Multi-Objective Optimal Torque Control with Simultaneous Motion and Force Tracking for Hydraulic Quadruped Robots},
	JOURNAL = {Machines},
	VOLUME = {10},
	YEAR = {2022},
	NUMBER = {3},
	ARTICLE-NUMBER = {170},
	URL = {https://www.mdpi.com/2075-1702/10/3/170},
	ISSN = {2075-1702},
	ABSTRACT = {Model-based force control for motion and force tracking faces significant challenges on real quadruped platforms due to the apparent model inaccuracies. In this paper, we present a multi-objective optimal torque control for hydraulic quadruped robots under significant model errors, such as non-modelable hydraulic components, linearization, disturbances, etc. More specifically, the centroidal dynamics are first modeled to project the dynamics of the floating-based whole-body behaviors to the centroidal frame. Model error compensation mechanisms are subsequently developed to track the reference motion of the CoM, torso, and foot-end trajectories, which are mapped into the joint space. Furthermore, a multi-objective optimal torque control scheme is formulated using quadratic programming (QP) to coordinate follow the reference motion and ground reaction forces simultaneously while satisfying all constraints. Finally, we present a series of simulations as well as experiments on a real hydraulic quadruped platform, EHbot. The results demonstrate that the proposed torque control scheme is robust to large model inaccuracies and improves the performance of the overall system.},
	DOI = {10.3390/machines10030170}}

@article{goo2022jdsmc,
	author = {Goo, Anthony and Laubscher, Curt and Sawicki, Jerzy T.},
	title = "{Hybrid Zero Dynamics Control of an Underactuated Lower-limb Exoskeleton for Gait Guidance}",
	journal = {Journal of Dynamic Systems, Measurement, and Control},
	year = {2022},
	month = {02},
	abstract = "{Hybrid zero dynamics (HZD) control creates dynamically stable gaits by driving a hybrid robot model towards an optimally generated state-dependent reference signal. This paper presents the first application of HZD to an underactuated lower-limb exoskeleton for gait guidance. A phase-based reference gait is generated to follow a nominal walking pattern. Simulation results using a phase-based proportional-derivative (PD) controller validate that stable periodic gait similar to nominal gait is possible for the identified human-exoskeleton model. For hardware application, a bilateral mixing strategy is taken to accommodate the presence of the double support phase which was assumed to be instantaneous in the hybrid model. A treadmill experiment is conducted with a healthy subject using the Indego Explorer exoskeleton. Comparing the recorded gait to the optimal phase-based reference, the root mean square joint tracking errors (RMSE) are 2.87°, 2.79°, 3.20°, and 11.0° at the stance hip, stance knee, swing hip, and swing knee, respectively. Similarly, compared to the commanded reference, the RMSE are 2.04°, 4.40°, 4.58°, and 6.83°, respectively. A second experiment was conducted wherein the treadmill speed varied from 0.3 to 0.5 m/s. The results show how the HZD controller exhibits remarkable flexibility and robustness for multiple walking speeds and grants the operator greater level of volitional control due to the time-invariant, phase-based nature of the implementation. Taken altogether, the presented results suggest that HZD control can serve as a rehabilitative control method capable of providing gait guidance in underactuated exoskeleton systems.}",
	issn = {0022-0434},
	doi = {10.1115/1.4053946},
	url = {https://doi.org/10.1115/1.4053946},
	eprint = {https://asmedigitalcollection.asme.org/dynamicsystems/article-pdf/doi/10.1115/1.4053946/6853886/ds-21-1273.pdf}}

